        <script type="text/javascript">

            $(document).ready(function () { BindTreeMenu(); })

            function BindTreeMenu() {
                $.ajax({
                    url: 'Home/GetTreeMenu',
                    method: 'POST',
                    data: "",
                    dataType: 'json',
                    contentType: 'application/json',
                    success: function (data) {
                        $('#TreeMenu').html("");
                        $('#TreeMenu').empty();
                        BuildMenu($('#TreeMenu'), data);
                    },
                    error: function (data) {
                    }
                });
            }
            function BuildMenu(parent, items) {
                $.each(items, function () {
                    if (this.MenuList && this.MenuList.length > 0)
                        var abc = "<li class='treeview'> <a href='javascript:void(0);'  onclick= \"" + this.Action + "\"   ><i class='fa fa-share'></i><span>" + this.MenuName + "</span><span class='pull-right-container'><i class='fa fa-angle-left pull-right'></i></span></a></li>";
                    else
                        var abc = "<li> <a href='javascript:void(0);' onclick= \"javascript: OpenPartialView('" + this.Action + "')\"  ><i class='fa fa-edit'> </i><span>" + this.MenuName + "</span></a></li>";

                    var li = $(abc);

                    if (!this.Active)
                        li.addClass('');

                    li.appendTo(parent);

                    if (this.MenuList && this.MenuList.length > 0) {
                        var ul = $("<ul class='treeview-menu' ></ul>");
                        ul.appendTo(li);
                        BuildMenu(ul, this.MenuList);
                    }
                });
            }
            function OpenPartialView(param) {

                $('.page-loader-wrapper').show();
                var ViewName = null;
                ViewName = param;
                $.ajax({
                    type: "Post",
                    url: "Home/GetPatialViewByName",
                    data: { ViewName: ViewName },
                    datatype: "html",
                    success: function (data) {

                        $('#LodeView').html('');
                        $('#LodeView').html(data);

                        $('.ls-closed').removeClass('overlay-open');
                        $(".overlay").css("display", "none");
                        $('.page-loader-wrapper').hide();
                    }
                })
            }
            function validation() {

                var Ret = true;

                $('.select2').each(function () {
                    if ($(this).val() == '0') {
                        $(this).css('border', 'solid 1px red')
                        $(this).attr('title', 'please select value')
                        Ret = false;
                    } else {
                        $(this).css('border', '')
                        $(this).attr('title', '')
                    }
                })

                $('[type=Text]').each(function () {
                    if ($(this).val() == '' && $(this).attr('name') != 'q') {
                        $(this).css('border', 'solid 1px red')
                        $(this).attr('title', 'please input value') 
                        Ret = false;
                    }
                    else {
                        $(this).css('border', '') 
                        $(this).removeAttr('title')
                    }
                })

                return Ret;

            }
            function ResetControl() {

                $('input').each(function () {

                    var Control = $(this).attr('type');

                    if (Control == "text")
                        $(this).val('')
                    else if (Control == "checkbox")
                        $(this).prop('checked', false)

                    $('.Rowid').text('0')

                })

            }

        </script>
